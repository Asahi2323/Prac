#include <stdio.h>
#include <time.h>
#include <stdlib.h>

int main(void){
    int n;
    srand((unsigned)time(NULL));
    n = rand() % 3 + 1;
    printf("Your Number is ...%d\n",n);
    
   switch(n){
      case 1 :
         printf("Yay!Todai iz berry hoppy!!");
         break;
      case 2 :
         printf("sawsaw");
         break;
      case 3 :
         printf("Nono,your tooooo baaaadd");
         break;
   }
   
}
